{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "drizzle"
    ],
    "rules": {
        "no-console": 0,
        "import/extensions": 0,
        "import/no-unresolved": 0,
        "drizzle/enforce-delete-with-where": "error",
        "drizzle/enforce-update-with-where": "error"
    }
}
